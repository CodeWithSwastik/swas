num => 101 &&&
counter => 1 &&&

while num - counter 
do
    if counter % 3 == 0 =>
        if counter % 5 == 0 =>
            upload "FizzBuzz" 
        else =>
            upload "Fizz"
    else =>
        if counter % 5 == 0 =>
            upload "Buzz"
        else =>
            upload counter

    &&&
    inc counter
